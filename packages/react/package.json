{
  "name": "@yllet/react",
  "private": false,
  "version": "2.1.0-alpha.3",
  "repository": "ylletjs/yllet",
  "description": "React bindings for Yllet",
  "author": "Fredrik Forsmo <fredrik.forsmo@gmail.com>",
  "keywords": [
    "wordpress",
    "rest",
    "api",
    "client",
    "react",
    "isomorphic",
    "headless"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "lib/",
    "types/"
  ],
  "bugs": {
    "url": "https://github.com/ylletjs/yllet/issues"
  },
  "scripts": {
    "build": "yarn declaration:build && yarn build:cjs && yarn build:esm",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./lib/cjs",
    "build:esm": "NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./lib/esm",
    "declaration:build": "rimraf types && tsc --outDir types -p declaration.tsconfig.json",
    "declaration:check": "git diff --exit-code -- index.d.ts",
    "prebuild": "rimraf lib",
    "preversion": "yarn run build",
    "tsc": "tsc types/index.d.ts"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.17",
    "@yllet/client": "^2.1.0-alpha.3",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "license": "MIT",
  "gitHead": "a2a2a3d8db1a79d619928f935f18c1bd05c8622d",
  "publishConfig": {
    "access": "public"
  }
}
